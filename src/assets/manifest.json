{
  "manifest_version": 3,
  "name": "Reading List Register",
  "short_name": "Reading List Register",
  "version": "0.1.0",
  "description": "Add active tab to the Reading List with shortcuts.",
  "author": "pHo9UBenaA",
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Triggering add tab to reading list"
    }
  },
  "permissions": ["readingList", "tabs"],
  "incognito": "spanning"
}
